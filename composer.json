{
    "name": "kerkness/fa-wired",
    "description": "Fathom Analytic Events for Laravel/Livewire",
    "keywords": ["laravel", "livewire", "alpine", "fathom", "analytics", "tracking"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kerkness",
            "email": "ryan@kerkness.ca"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kerkness\\FaWired\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerkness\\FaWired\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kerkness\\FaWired\\FaWiredServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}